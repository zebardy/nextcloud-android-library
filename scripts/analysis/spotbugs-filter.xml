<?xml version="1.0" encoding="UTF-8"?>


<FindBugsFilter>
    <Match>
        <Class name="~.*\.Manifest\$.*" />
    </Match>
    <Match>
        <Class name="~.*\.R\$.*" />
    </Match>
    <Match>
        <Class name="~.*\$\$Parcelable.*" />
    </Match>
    <Bug pattern="PATH_TRAVERSAL_IN" />
    <Bug pattern="ANDROID_EXTERNAL_FILE_ACCESS" />
    <Bug pattern="BAS_BLOATED_ASSIGNMENT_SCOPE" />
    <Bug pattern="HTTP_PARAMETER_POLLUTION" />
</FindBugsFilter>
